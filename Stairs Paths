#include <bits/stdc++.h>
using namespace std;

vector<string> f(int n)
{
    if(n == 0)
    return {""};
    
    else if(n < 0)
    return {};
    
  vector<string> p1= f(n-1);
  vector<string> p2= f(n-2);
  vector<string> p3= f(n-3);
   
   vector<string> ans;
   
   for(auto &it: p1)
   ans.push_back('1'+it);
   
   for(auto &it: p2)
   ans.push_back('2'+it);
   
   for(auto &it: p3)
   ans.push_back('3'+it);
   
   return ans;
    
}
int main()
{
  
  auto v= f(5);
  
  for(auto vec: v)
    cout<<vec<<" ";

}
